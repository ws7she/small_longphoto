.complex {
  position: relative;
  width: 100%;
  height: 100%;
  background-color: #eee;
}

.complex-func {
  position: fixed;
  left: 0;
  bottom: 0;
  width: 100%;
  display: flex;
  display: -webkit-flex;
  background-color: #fff;
  border-top: 1px solid #e6e6e6;
}

.complex-func view {
  flex: 1;
  -webkit-flex: 1;
  text-align: center;
  color: #666;
  line-height: 30px;
}

.edit-border {
  border: 1px solid red;
  position: absolute;
  top: 10px;
  left: 10px;
}

.edit-border .iconfont {
  position: absolute;
}

.delete {
  left: -10px;
  top: -10px;
}

.scale {
  right: -10px;
  bottom: -10px;
}

.translate {
  top: -10px;
  right: -10px;
}

.cover {
  position: fixed;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
  background-color: rgba(0, 0, 0, 0.5);
}

.edit-all {
  width: 250px;
  position: absolute;
  left: 50%;
  top: 50%;
  transform: translate(-50%, -80%);
}

.edit-main {
  width: 100%;
  padding: 7px;
  box-sizing: border-box;
  min-height: 150px;
  background-color: #fff;
}

.color-main {
  width: 100%;
  display: flex;
  display: -webkit-flex;
  background: #eee;
  height: 40px;
  border-top: 1px solid transparent;
}

.color-main .colorChoose {
  flex: 1;
  -webkit-flex: 1;
  text-align: center;
  height: 30px;
  border-radius: 50%;
  margin: 5px 6px;
}

.addIn {
  width: 100%;
  line-height: 35px;
  display: flex;
  display: -webkit-flex;
  background-color:#eee;
  border-top: 1px solid #fff;
}
.addIn view{
    flex: 1;
  -webkit-flex: 1;
  text-align: center;
  line-height: 35px;
}
.sure{
  color:red;

}
.cancel{
  color: #666;
  border-right:1px solid #fff;
}
.canvasMain view{
  transform-origin: left top;
}
.makePic{
  position: fixed;
  background-color: lightgreen;
  line-height: 30px;
  width: 50px;
  right: 10px;
  bottom: 50px;
  z-index: 100;
  text-align: center;
  color: #fff;
}